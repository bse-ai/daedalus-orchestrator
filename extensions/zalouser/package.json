{
  "name": "@forge-orchestrator/zalouser",
  "version": "2026.2.13",
  "description": "ForgeOrchestrator Zalo Personal Account plugin via zca-cli",
  "type": "module",
  "dependencies": {
    "@sinclair/typebox": "0.34.48"
  },
  "devDependencies": {
    "forge-orchestrator": "workspace:*"
  },
  "forge-orchestrator": {
    "extensions": [
      "./index.ts"
    ],
    "channel": {
      "id": "zalouser",
      "label": "Zalo Personal",
      "selectionLabel": "Zalo (Personal Account)",
      "docsPath": "/channels/zalouser",
      "docsLabel": "zalouser",
      "blurb": "Zalo personal account via QR code login.",
      "aliases": [
        "zlu"
      ],
      "order": 85,
      "quickstartAllowFrom": true
    },
    "install": {
      "npmSpec": "@forge-orchestrator/zalouser",
      "localPath": "extensions/zalouser",
      "defaultChoice": "npm"
    }
  }
}
